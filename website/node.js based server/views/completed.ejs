	</head>

	<body>
		<header class="navbar-inverse" role="banner">
			<!--<div class="container">-->
				<nav role="navigation">
				  <div class="container-fluid">
				    <!-- Brand and toggle get grouped for better mobile display -->
				    <div class="navbar-header">
				      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
				        <span class="sr-only">Toggle navigation</span>
				        <span class="icon-bar"></span>
				        <span class="icon-bar"></span>
				        <span class="icon-bar"></span>
				      </button>
				      <a class="navbar-brand" href="#">Canvas++</a>
				    </div>

				    <!-- Collect the nav links, forms, and other content for toggling -->
				    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
				      <ul class="nav navbar-nav">
				        <li><a href="todo">Tasks</a></li>
				        <li class="active"><a href="#">Completed</a><span class="sr-only">(current)</span></li>
				        <li><a href="index.html">Log Out</a></li>
				      </ul>

				    </div><!-- /.navbar-collapse -->
				  </div><!-- /.container-fluid -->
				</nav>

		</header>
		<div class="listContainer">
			<div class="heading"> Completed </div>
	    <ul id="todoList">
	      <!-- Allows to get the data list from the javascript controller -->
	      <% for(var i = 0; i < todos.length; i++){ %>
	        <div class="taskText">
	          <li id="onHover"><%= todos[i].item %></li>
	          <div class="showOnHover">
	            <a href="#" class="btn btn-xs btn-warning">Not Complete</a>
	            <a href="#" class="btn btn-xs btn-danger">Delete</a>
	          </div>
	        </div>
	      <% } %>
	    </ul>
		</div>
